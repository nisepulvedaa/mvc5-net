@using System.Web.Optimization
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Metronic/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/Metronic/assets/global/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <script src="~/Metronic/assets/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>

    <link href="~/Content/plugins/sweetalert/sweetalert.css" rel="stylesheet" type="text/css" />


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
               @* <a><span style="font-size:12px;color:#ffffff;text-align:center;position:relative;left:10px;">Hostal</span>  <br /><span style="font-size:12px;color:#ffffff;">Doña Claarita</span></a>*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><img src="~/Content/imagenes/hostal1.JPG" /> </li>
                    <li>@Html.ActionLink("Inicio ", "Index", "Home/Index")</li>
                    <li>@Html.ActionLink("¿Quienes Somos?", "Somos", "Home")</li>
                    <li>@Html.ActionLink("Servicios", "Servicios", "Home")</li>
                     <li>@Html.ActionLink("Contacto", "Contacto", "Home")</li>
                   
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li>@Html.ActionLink("Registrarse", "Register", "Home")</li>
                    <li>@Html.ActionLink("Iniciar Sesion", "Index", "Login/Index")</li>
                   
                </ul>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderSection("styles", false)
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Metronic/assets/global/plugins/jquery.min.js"></script>
    <script src="~/Metronic/assets/global/plugins/jquery-migrate.min.js"></script>
    <script src="~/Metronic/assets/global/plugins/jquery.cokie.min.js"></script>
    <script src="~/Metronic/assets/global/plugins/jquery.blockui.min.js"></script>
    <script src="~/Metronic/assets/global/plugins/uniform/jquery.uniform.min.js"></script>
    <script src="~/Metronic/assets/global/plugins/jquery-validation/js/jquery.validate.min.js"></script>
    <script src="~/Content/plugins/sweetalert/sweetalert.min.js"></script>
    <script src="~/Metronic/assets/global/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Metronic/assets/global/scripts/metronic.js"></script>

    <script src="~/Content/js/SAC.js"></script>

    @Scripts.Render("~/Scripts/Default")
    @RenderSection("scripts", false)
    <script>
        $(document).ready(function () {
            Metronic.init(); // init metronic core componets

        });
    </script>

</body>
</html>